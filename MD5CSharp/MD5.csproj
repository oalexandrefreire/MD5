<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Alexandre Freire</Authors>
    <Company>AF</Company>
    <PackageTags>Hash, MD5, MD5Hash, Encryptor</PackageTags>
    <Description>Encrypt strings easily.
Example:
string password = MD5Hash.Hash.Content("123");</Description>
    <PackageLicenseFile>assets/license.txt</PackageLicenseFile>
    <ApplicationIcon>assets/logo.ico</ApplicationIcon>
    <PackageIcon>assets/logo.png</PackageIcon>
    <PackageIconUrl />
    <RepositoryUrl>https://github.com/alexandrefreiref/MD5-CSharp</RepositoryUrl>
    <Version>2.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="assets/logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="assets/license.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
